        "slideHeadline": { "$invariant": "DXC" },
        "slideText": {
          "$invariant": "<p> </p>\n<p>“DXC combines the ability to safely innovate in total experience for business and development users, while relying on and retaining the integrity of the API-packaged underlying application base.”</p>\n<p style=\"float: right;\"><img src=\"https://res.cloudinary.com/uniform-solution-engineering/image/upload/v1663666813/Umbraco%20-%20Charlotte/Gartner_logo_yuqu5w.svg\" alt=\"\" /></p>"
