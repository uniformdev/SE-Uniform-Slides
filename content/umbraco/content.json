        "sortOrder": 4,
        "slideTitle": {
          "$invariant": "DXC puts a name to what Uniform has been building from the beginning"
        },
        "slideHeadline": { "$invariant": "" },
        "slideText": {
          "$invariant": "<p> </p>\n<ul>\n<li><strong>Uniform Mesh provides composable orchestration</strong> that acts as scaffolding for composable APIs, eliminating the need for complex custom integration code that doesn't create business value</li>\n<li><strong>Uniform Canvas lets marketers, merchandisers and other business users create digital experiences without code</strong> using content from multiple legacy and headless sources, flexible front-end components, forms and anything else a business needs</li>\n</ul>"
        }
